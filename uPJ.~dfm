  object IBPessoasVinculadas: TIBQuery
    Database = DM.Conexao
    Transaction = DM.IBTransaction
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      
        'select p.NOME, p.CPF, pv.FUNCAO, pv.CHAVE_PESSOAS_VINCULOS from ' +
        'PESSOAS_VINCULOS pv'
      'inner join pessoas p'
      'on p.chave_pessoas = pv.chave_pessoas')
    Left = 248
    Top = 16
  end
